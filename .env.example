# --- Database Settings ---
DATABASE_URL=postgresql://postgres:postgres@db:5432/sp500_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=sp500_db

# --- Application Mode ---
APP_MODE=DEMO